# Declare stack frame locations
register data_stack_pointer: u8 @ 0x0000;

# Initialize the stack
org 0xE000;

register output: u8 @ 0x0200;

data_stack_pointer = 3;
main();

def main(): void
    # Should output 21
    output = test_fn(1, 2, 3) + test_fn(4, 5, 6);
    goto halt;
end

def test_fn(a: u8, b: u8, c: u8): u8
    return a + b + c;
end

def halt(): void
    goto halt;
end